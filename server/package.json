  "scripts": {
    "test": "mocha -r ts-node/register --require ./test/test-setup.ts test/**/**.test.ts",
    "test:watch": "mocha -r ts-node/register --require ./test/test-setup.ts test/**/**.test.ts --watch",
    "compile": "node node_modules/typescript/bin/tsc",
